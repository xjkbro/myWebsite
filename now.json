{
    "name": "mywebsite",
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "server.js"
        }
    ],
    "env": {
        "MAILGUN_API_KEY": "@mailgun_api_key",
        "MAILGUN_DOMAIN": "@mailgun_domain"
    }
}